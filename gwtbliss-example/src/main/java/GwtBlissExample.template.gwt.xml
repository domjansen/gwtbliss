  <inherits name='${gwt.module.base}' />
  <set-configuration-property
    name="gwtp.prebootstrapper"
    value="${gwt.module.gwtp.prebootstrapper}" />
  <set-property
    name="gwt.logging.enabled"
    value="${gwt.module.logging.enabled}" />
  <set-property
    name="gwt.logging.logLevel"
    value="${gwt.module.logging.logLevel}" />
  <set-property
    name="gwt.logging.consoleHandler"
    value="${gwt.module.logging.localLogging}" />
  <set-property
    name="gwt.logging.developmentModeHandler"
    value="${gwt.module.logging.localLogging}" />
  <set-property
    name="gwt.logging.systemHandler"
    value="${gwt.module.logging.localLogging}" />
  <set-property
    name="gwt.logging.simpleRemoteHandler"
    value="${gwt.module.logging.remoteLogging}" />
  <set-property
    name="compiler.useSourceMaps"
    value="${gwt.module.compiler.useSourceMaps}" />
  <set-property
    name="compiler.stackMode"
    value="${gwt.module.compiler.stackMode}" />
  <set-property
    name="gwt.enableDebugId"
    value="${gwt.module.gwt.enableDebugId}" />
  <set-configuration-property
    name="compiler.enum.obfuscate.names"
    value="${gwt.module.compiler.enum.obfuscate.names}" />
  <set-configuration-property
    name="CssResource.style"
    value="${gwt.module.CssResource.style}" />
  <set-configuration-property
    name="devModeRedirectEnabled"
    value="${gwt.module.devModeRedirectEnabled}" />
  <set-property
    name="user.agent"
    value="${gwt.module.user.agent}" />
  <set-property
    name="locale"
    value="${gwt.module.locale}" />

</module>
